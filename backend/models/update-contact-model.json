{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "minItems": 3
    },
    "phoneNumber": {
      "type": "string",
      "minLength": 8
    },
    "address": {
      "type": "string",
      "minLength": 3,
      "minItems": 3
    },
    "dueDate": {
      "type": "string"
    },
    "favorite": {
      "type": "boolean"
    }
  },
  "required": [
    "name",
    "dueDate",
    "favorite"
  ],
  "additionalProperties": false
}
